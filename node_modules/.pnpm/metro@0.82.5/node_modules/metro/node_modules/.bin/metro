#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules/metro/src/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules/metro/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules/metro/src/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules/metro/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/metro@0.82.5/node_modules:/Volumes/Mac-Data/Users/adnanbaig/Development/GitHub/muay-thai/nak-muay-trainer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli.js" "$@"
else
  exec node  "$basedir/../../src/cli.js" "$@"
fi
